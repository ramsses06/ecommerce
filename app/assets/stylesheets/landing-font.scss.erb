@font-face{
	font-family:"Kostic";
	src: url(asset_path("Kostic-Regular.ttf")) format('opentype');
}

@font-face{
	font-family:"Kostic";
	src: url(asset_path("Kostic-Bold.ttf")) format('truetype');
	font-weight: bold;
}

@font-face{
	font-family:"Kostic";
	src: url(asset_path("Kostic-Thin.ttf")) format('truetype');
	font-weight: 100;
}

.kostic-regular{
	font-family: "Kostic";
	font-weight: normal;
}
.kostic-bold{
	font-family: "Kostic";
	font-weight: bold;
}
.kostic-lighter{
	font-family: "Kostic";
	font-weight: lighter;
}
